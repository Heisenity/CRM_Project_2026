{
  "name": "@crmdemo/backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "_moduleAliases": {
    "@": "dist"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc && node -e \"const fs=require('fs'); const path=require('path'); fs.mkdirSync('dist/swagger', {recursive: true}); fs.copyFileSync('src/swagger/openapi.yaml', 'dist/swagger/openapi.yaml');\"",
    "start": "node -r ./module-alias-register.js dist/index.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "db:start": "docker-compose up -d mysql",
    "db:stop": "docker-compose down",
    "db:restart": "docker-compose restart mysql",
    "db:logs": "docker-compose logs -f mysql",
    "db:reset": "docker-compose down -v && docker-compose up -d mysql",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "seed:admin": "tsx scripts/seed-deployment-admin.ts",
    "seed:admin:js": "node scripts/seed-deployment-admin.js",
    "cleanup:credentials": "node scripts/cleanup-credentials.js",
    "cleanup:credentials:force": "node scripts/cleanup-credentials.js --force",
    "migrate:production": "node scripts/migrate-production.js",
    "force-migrate": "node scripts/force-migrate.js",
    "deploy": "npm run build && npm run seed:admin:js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/s3-request-presigner": "^3.980.0",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@prisma/adapter-pg": "^5.19.0",
    "@prisma/client": "^5.19.0",
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.4",
    "@types/puppeteer": "^5.4.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bwip-js": "^4.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "fs-extra": "^11.3.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.2",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.18.0",
    "puppeteer": "^21.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-model-validator": "^3.0.21",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.8",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bwip-js": "^3.2.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^30.2.0",
    "prisma": "^5.19.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "types": "./dist/index.d.ts"
}
